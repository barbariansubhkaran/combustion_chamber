FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    fuel
    {
      type            flowRateInletVelocity;
      massFlowRate    constant 0.00011667;
      rhoInlet        1.1965;
      value           uniform (0 0 0);
    }
    air
    {
      type            flowRateInletVelocity;
      massFlowRate    constant 0.009545;
      rhoInlet        1.1965;
      value           uniform (0 0 0);
    }

    secondary_air
    {
      type            flowRateInletVelocity;
      massFlowRate    constant 0.00075945;
      rhoInlet        1.1965;
      value           uniform (0 0 0);
    }

    outlet
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }

    wallTube
    {
        type            noSlip;
    }
    wallOutside
    {
        type            zeroGradient;
    }
}


// ************************************************************************* //
