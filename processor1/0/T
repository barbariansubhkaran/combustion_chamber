{
    wallOutside
    {
        type            zeroGradient;
    }
    inletAir
    {
        type            fixedValue;
        value           uniform 291;
    }
    outlet
    {
        type            zeroGradient;
    }
    inletCH4
    {
        type            fixedValue;
        value           uniform 294;
    }
    inletPilot
    {
        type            fixedValue;
        value           uniform 1880;
    }
    wallTube
    {
        type            zeroGradient;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 300;
    }
}


