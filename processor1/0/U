FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0.9);

boundaryField
{
    wallOutside
    {
        type            zeroGradient;
    }
    inletAir
    {
        type            fixedValue;
        value           uniform (0 0 -20);
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        value           nonuniform 
0
;
    }
    inletCH4
    {
        type            fixedValue;
        value           uniform (0 0 -49.6);
    }
    inletPilot
    {
        type            fixedValue;
        value           uniform (0 0 -20);
    }
    wallTube
    {
        type            noSlip;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 0 0.9);
    }
}


